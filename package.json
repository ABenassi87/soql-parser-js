{
  "name": "soql-parser-js",
  "version": "0.1.3",
  "description": "Salesforce.com SOQL parser.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "prebuild2": "npm run antlr; npm run clean",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "tsc": "./node_modules/.bin/tsc",
    "preantlr": "rm -rf src/generated/*",
    "antlr": "./node_modules/.bin/antlr4ts -visitor SOQL.g4 -o src/generated/",
    "publish": "npm run build && npm publish"
  },
  "author": "Austin Turner <paustint@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "antlr4ts": "0.4.1-alpha.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "8.0.14",
    "@types/lodash": "^4.14.116",
    "antlr4ts-cli": "0.4.0-alpha.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typescript": "2.4.1"
  },
  "keywords": [
    "soql",
    "Salesforce"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paustint/soql-parser-js"
  }
}
